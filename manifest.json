{
  "name": "ScreenShots",
  "version": "1.0",
  "description": "Capture screenshot and open in new tab",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "camera.png",
    "default_title": "Screenshot!!"
  },
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 2
}

